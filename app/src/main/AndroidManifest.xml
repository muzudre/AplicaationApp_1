<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muzud.aplicaationapp_1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Lectures"
            android:label="Lectures"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".TimeTable"
            android:label="Time Table"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme3" />
        <activity
            android:name=".Modules"
            android:label="Modules"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme4" />
        <activity
            android:name=".Portal"
            android:label="Portal"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme4" />
        <activity
            android:name=".Facebook"
            android:label="Facebook"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme4" />
        <activity
            android:name=".Instagram"
            android:label="Instagram"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme4" />
        <activity
            android:name=".Twitter"
            android:label="Twitter"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme4" />
        <activity
            android:name=".Linkedin"
            android:label="Linkedin"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme4" />
        <activity android:name=".Location" />
        <activity android:name=".Contacts"></activity>
    </application>

</manifest>